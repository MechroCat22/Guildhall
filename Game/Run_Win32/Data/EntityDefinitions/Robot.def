<EntityDefinitions>

	<EntityDefinition name="Player" dimensions="8,8,8">

		<!--Movement Parameters-->
		<Movement maxAcceleration="300.0" maxSpeed="40.0" maxDeceleration="100.0" jumpImpulse="80.0"/>

		<!--Visuals-->
		<Animation set="Robot" defaultSprite="Robot_idle_0"/>

		<!--Physics-->
		<Physics physicsType="dynamic" hasGravity="true">
			<CollisionDefinition shape="disc" response="share_correction" xExtent="4.0" zExtent="4.0" height="8.0"/>
		</Physics>

		<!--AI-->

	</EntityDefinition>

		<EntityDefinition name="Bullet" dimensions="1,1,1">

		<!--Visuals-->
		<Animation defaultSprite="Bullet"/>

		<!--Physics-->
		<Physics physicsType="dynamic" hasGravity="false">
			<CollisionDefinition shape="box" response="ignore_correction" teamException="same" xExtent="0.5" zExtent="0.5" height="1.0" layer="1"/>
		</Physics>

	</EntityDefinition>

	<EntityDefinition name="Robot" dimensions="8,8,8">

		<!--Movement Parameters-->
		<Movement maxAcceleration="3000.0" maxSpeed="20.0" maxDeceleration="100.0" jumpImpulse="120.0"/>

		<!--Visuals-->
		<Animation set="Robot" defaultSprite="Robot_idle_0"/>

		<!--Physics-->
		<Physics physicsType="dynamic" hasGravity="true">
			<CollisionDefinition shape="disc" response="share_correction" xExtent="4.0" zExtent="4.0" height="8.0"/>
		</Physics>

		<!--AI-->
		<AI>
			<!--<Shoot projectile="Bullet" fireRate="1.0"/>-->
			<PursueDirect/>
		</AI>

	</EntityDefinition>

	<EntityDefinition name="Wall" dimensions="8,8,8">

		<!--Visuals-->
		<Animation defaultSprite="Wall"/>

		<!--Physics-->
		<Physics physicsType="static">
			<CollisionDefinition shape="box" response="no_correction" xExtent="4.0" zExtent="4.0" height="8.0"/>
		</Physics>

	</EntityDefinition>


	<EntityDefinition name="Particle" dimensions="1,1,1">

		<!--Physics-->
		<Physics physicsType="dynamic" hasGravity="true">
			<CollisionDefinition shape="box" response="full_correction" xExtent="0.5" zExtent="0.5" height="1.0"/>
		</Physics>

		<!--AI-->

	</EntityDefinition>

</EntityDefinitions>